<?xml version='1.0' encoding='UTF-8' standalone='no'?>
<qnx xmlns="http://www.qnx.com/schemas/application/1.0">
    <id>org.qremote.qremotecontrol-client</id>
    <name>QRemoteControl Client</name>
    <versionNumber>2.4.0</versionNumber>
    <icon><image>qml/QRemoteControl2/images/qrc.png</image></icon>
    <description>With QRemoteControl you can remotely control your computer.</description>
    <initialWindow>
        <aspectRatio>landscape</aspectRatio>
        <autoOrients>false</autoOrients>
        <systemChrome>none</systemChrome>
        <transparent>false</transparent>
    </initialWindow>
    <author>qremote.org</author>
    <copyright>Copyright 2012-2013 by Alexander RÃ¶ssler</copyright>
    <env value="app/native/imports:/usr/lib/qt4/imports" var="QML_IMPORT_PATH"/>
    <env value="app/native/plugins:/usr/lib/qt4/plugins" var="QT_PLUGIN_PATH"/>
    <env value="app/native/lib:/usr/lib/qt4/lib/" var="LD_LIBRARY_PATH"/>
    <arg>-style</arg>
    <arg>qnxlight</arg>
    <action system="true">run_native</action>
    <!-- qremotecontrol2 should point to the project binary, path can be relative -->
    <asset path="qremotecontrol2" type="Qnx/Elf" entry="true">qremotecontrol2</asset>
    <asset path="%SRC_DIR%/qml">qml</asset>
    <authorId>gYAAgFPlfKdPddZH5EdJnaAtSGI</authorId>
</qnx>
